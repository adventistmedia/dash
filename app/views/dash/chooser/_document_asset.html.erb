<%= content_tag_for(:tr, asset, class: "asset-document") do %>
  <td><%= asset.content_type.split('/').last.upcase %>: <%= asset.name %></td>
  <td><%= l(asset.created_at.to_date) %></td>
  <td>
    <%= link_to t("dash.add"), "#", class: "chooser-file-insert btn btn-secondary btn-sm", data: asset.chooser_data %>
  </td>
<% end %>