<%= render "/dash/chooser/nav", menu: :images %>
<div class="container-fluid">
  <div class="row">
    <div class="col-12">
      <%= table_overview(header_actions: [], search: true) do %>
        <%= link_to "Upload Image", chooser_path(:upload_image), class: "btn btn-primary", data: {turbolinks: false} %>
      <% end %>
      <%= table_filters(@assets) %>
      <div id="asset-image-list">
        <%= render collection: @assets, partial: "/dash/chooser/image_asset", as: :asset %>
      </div>
      <%= table_paginate @assets %>
    </div>
  </div>
</div>